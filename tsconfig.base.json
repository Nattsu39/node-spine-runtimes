{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"inlineSourceMap": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"lib": ["ESNEXT"],
		"declaration": true,
		"composite": true,
		"moduleResolution": "node",
		"skipLibCheck": true,
		"strict": true
	}
}
